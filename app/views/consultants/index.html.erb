


<div class="row">
  <div class="col-sm-6">
    <h1>Consultants</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_consultant_path, class: 'btn btn-primary' do %>
    Add New Consultant
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>First name</th>
            <th>Last name</th>
            <th>Phone</th>
            <th>Bio</th>
            <th>User</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @consultants.each do |consultant| %>
        <%= content_tag :tr, id: dom_id(consultant), class: dom_class(consultant) do %>
          

                      <td><%= consultant.first_name %></td>
                      <td><%= consultant.last_name %></td>
                      <td><%= consultant.phone %></td>
                      <td><%= consultant.bio %></td>
                      <td><%= consultant.user %></td>
          
          
            <td><%= link_to 'Show', consultant %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
