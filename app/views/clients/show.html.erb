<div class="page-header">
  <%= link_to clients_path, class: 'btn btn-default' do %>
    All Clients
  <% end %>
  <%= link_to edit_client_path(@client), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show client</h1>
</div>

<dl class="dl-horizontal">
  <dt>First name:</dt>
  <dd><%= @client.first_name %></dd>

  <dt>Last name:</dt>
  <dd><%= @client.last_name %></dd>

  <dt>Phone:</dt>
  <dd><%= @client.phone %></dd>

  <dt>Bio:</dt>
  <dd><%= @client.bio %></dd>

  <dt>User:</dt>
  <dd><%= @client.user %></dd>

</dl>
